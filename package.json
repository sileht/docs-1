    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-mdx": "^3.0.0",
    "eslint-plugin-mdx": "^3.0.0",
    "prettier": "^3.1.1",
    "remark-preset-lint-markdown-style-guide": "^5.1.3",
    "typescript": "^5.3.3"
