    "gatsby-plugin-react-helmet": "^6.12.0",
    "gatsby-plugin-sitemap": "^6.12.3",
    "gatsby-plugin-svgr": "^3.0.0-beta.0",
    "gatsby-plugin-webfonts": "^2.3.2",
    "gatsby-remark-autolink-headers": "^6.13.0",
